    "url": "https://github.com/johnfyoung/scssstarter/issues"
  },
  "homepage": "https://github.com/johnfyoung/scssstarter#readme",
  "devDependencies": {
    "@ungap/url-search-params": "^0.2.2",
    "copy-webpack-plugin": "^10.2.0",
    "file-loader": "^6.2.0",
    "sass": "^1.45.1",
    "sass-loader": "^12.4.0",
    "uglifycss": "^0.0.29",
    "webpack": "^5.65.0",
    "webpack-cli": "^4.9.1"
  }
